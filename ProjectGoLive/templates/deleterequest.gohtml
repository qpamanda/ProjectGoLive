{{template "header" .User}}

<h2>Delete an Existing Request</h2>

{{if eq .CntCurrentItems 0}}
    <p style="color:red;">No requests have been made.</p>
{{else}}
<p style="color:red;">{{.ClientMsg}} </p> 

{{ if not .FormSubmitted}}
{{ template "delreq-form" .}}
{{else}}
{{ template "delreq-complete" .}}
{{end}}
{{template "footer"}}

{{end}}
</body>
</html>